print("Independent Cascade Example") defic(G,s): print(s) jst_inf=list(s) infected=list(s) while(1): print(jst_inf, infected) iflen(jst_inf)==0: returninfected tmp=[] foreachinjst_inf: foreach1inG.neighbors(each): r=random.uniform(0,1) ifr<0.5andeach1notininfectedandeach1notintmp: tmp.append(each1) foreachintmp: infected.append(each) jst_inf=list(tmp)
